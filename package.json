    "dynamoose": "^1.11.1",
    "node-uuid": "^1.4.8",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
